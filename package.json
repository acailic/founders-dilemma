{
  "name": "founders-dilemma",
  "version": "0.2.5",
  "description": "An educational startup simulation game that teaches real founder principles through gameplay",
  "type": "module",
  "private": false,
  "license": "MIT",
  "author": "Founder's Dilemma Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/acailic/founders-dilemma.git"
  },
  "homepage": "https://acailic.github.io/founders-dilemma/",
  "bugs": {
    "url": "https://github.com/acailic/founders-dilemma/issues"
  },
  "keywords": [
    "game",
    "startup",
    "simulation",
    "educational",
    "tauri",
    "react"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@fontsource/open-sans": "^5.2.7",
    "@mantine/core": "^8.3.4",
    "@mantine/hooks": "^8.3.4",
    "@mantine/modals": "^8.3.4",
    "@mantine/notifications": "^8.3.4",
    "@tabler/icons-react": "^3.35.0",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-autostart": "^2.5.0",
    "@tauri-apps/plugin-deep-link": "^2.4.3",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-fs": "~2.2.0",
    "@tauri-apps/plugin-log": "~2.2.1",
    "@tauri-apps/plugin-notification": "~2.2.3",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "@tauri-apps/plugin-os": "^2.3.1",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-shell": "^2.3.1",
    "@tauri-apps/plugin-store": "^2.4.0",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "dayjs": "^1.11.18",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "js-cookie": "^3.0.5",
    "localforage": "^1.10.0",
    "mutative": "^1.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^4.1.2",
    "react-i18next": "^15.7.4",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "react-transition-group": "^4.4.5",
    "simplebar-react": "^3.3.2",
    "typescript": "^5.9.3",
    "use-mutative": "^1.3.1"
  },
  "scripts": {
    "dev": "tauri dev",
    "nvmrc": "node -v > .nvmrc",
    "postinstall": "pnpm nvmrc",
    "predev": "pnpm nvmrc",
    "tauri": "tauri",
    "start": "vite",
    "rls": "dotenv-load tauri build",
    "build": "vite build",
    "build:web": "cross-env VITE_BASE_PATH=/founders-dilemma vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "preinstall": "echo 'Skipping global package installation'",
    "sloc": "sloc src src-tauri/src",
    "upgrade": "concurrently \"pnpm up\" \"cd src-tauri && cargo update && cargo clean\"",
    "test": "mocha",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "type-check": "tsc --noEmit",
    "clippy": "cd src-tauri && cargo clippy --all-targets --all-features -- -D warnings",
    "fmt": "cd src-tauri && cargo fmt --all",
    "fmt:check": "cd src-tauri && cargo fmt --all -- --check",
    "test:rust": "cd src-tauri && cargo test --all-features",
    "check": "pnpm type-check && pnpm lint && pnpm format:check && pnpm fmt:check",
    "check:fix": "pnpm lint:fix && pnpm format && pnpm fmt",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.8.4",
    "@types/js-cookie": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^4.7.0",
    "chai": "^5.3.3",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "dotenv-load": "^3.0.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "mocha": "^11.7.4",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "selenium-webdriver": "^4.36.0",
    "terser": "^5.44.0",
    "toml": "^3.0.0",
    "vite": "^7.2.2",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-top-level-await": "^1.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ],
    "src-tauri/src/**/*.rs": [
      "cargo fmt --"
    ]
  },
  "pnpm": {
    "overrides": {
      "js-yaml": ">=4.1.1"
    }
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
